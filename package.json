{
  "name": "production-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack --env mode=development",
    "start": "webpack serve --env port=3000 --env mode=development",
    "build:prod": "webpack --env mode=production",
    "lint:ts": "eslint '**/*.{ts,tsx}'",
    "lint:ts:fix": "eslint '**/*.{ts,tsx}' --fix",
    "lint:scss": "npx stylelint '**/*.scss'",
    "lint:scss:fix": "npx stylelint '**/*.scss' --fix",
    "test": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test --host 192.168.1.106 --port 56078",
    "test:ui:ci": "npx loki ---reactPort 56078 --requireReference --reactUri file:./storybook-static",
    "test:ui:approve": "npx loki approve",
    "storybook": "storybook dev -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "visual:report": "npm run visual:report:json && npm run visual:report:html",
    "visual:report:json": "node scripts/generate-visual-json-report.js",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "start:server": "node ./json-server/index.js",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
